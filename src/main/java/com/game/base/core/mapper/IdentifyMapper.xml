<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
  <mapper namespace="com.game.base.core.mapper.IdentifyMapper" >
  <resultMap id="BaseResultMap" type="com.game.base.core.entity.IdentifyEnt" >
    <id column="identify_id" property="identifyId" jdbcType="BIGINT" />
    <result column="type_id" property="typeId" jdbcType="INT" />
  </resultMap>

  <select id="selectIdentifyEnt" resultMap="BaseResultMap" parameterType="java.lang.Long" >
    select *
    from identify
    where identify_id = #{identifyId,jdbcType=BIGINT}
  </select>
  
  <insert id="insertIdentifyEnt" parameterType="com.game.base.core.entity.IdentifyEnt" >
    insert into identify (identify_id, typeId)
    values (#{identifyId,jdbcType=BIGINT}, #{typeId,jdbcType=INT} )
  </insert>


</mapper>